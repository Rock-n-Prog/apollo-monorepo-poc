datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
  output          = "../../../node_modules/.prisma/client"
}

model Content {
  id            String    @id @default(cuid())
  title         String
  year          Int
}
