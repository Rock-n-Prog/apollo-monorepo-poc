{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@acme/web#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "env": ["NODE_ENV", "VERCEL_URL", "RENDER_INTERNAL_HOSTNAME", "PORT"]
    },
    "@acme/api-gateway#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": ["CONTENTS_URL", "REVIEWS_URL"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "generate": {
      "outputs": []
    },
    "migrate:dev": {
      "dependsOn": ["^generate"],
      "env": ["DATABASE_URL"],
      "outputs": []
    },
    "seed": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "env:local": {
      "outputs": []
    },
    "storybook": {
      "outputs": []
    }
  }
}
